#!/usr/bin/env node


//-----åŒæ­¥å†™æ³•ðŸ‘‡
const execSync=require('child_process').execSync

const res=execSync('ls')
console.log("res", res.toString())


// ----- å¼‚æ­¥å†™æ³•ðŸ‘‡
const exec=require('child_process').exec
exec('ls',(err,stdout,stderr)=>{
   if(err)throw err
   console.log(stdout)
})